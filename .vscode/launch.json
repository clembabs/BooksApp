    "version": "0.2.0",
    "configurations": [
      {
        "name": "Run Dev App",
        "program": "lib/main.dart",
        "request": "launch",
        "type": "dart",
        
      },
      {
        "name": "Run All Tests",
        "type": "dart",
        "request": "launch",
        "program": "./test/"
      },
      {
        "name": "Run Storybook",
        "program": "lib/storybook/main_story.dart",
        "request": "launch",
        "type": "dart"
      },
      {
        "name": "Run Build Runner",
        "request": "launch",
        "program": ".dart_tool/build/entrypoint/build.dart",
        "type": "dart",
        "args": ["build", "--delete-conflicting-outputs"]
      }
    ]
  }
  